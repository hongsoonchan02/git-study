CREATE TABLE member_notnull (
	id varchar(50) NOT NULL,
	pwd varchar(250) NOT NULL,
	name varchar(50),
	age int,
	in_date datetime
	);

INSERT INTO member_notnull
values(NULL, NULL, '홍순찬', 25, now());

INSERT INTO member_notnull(id, pwd, in_date)
VALUES ('test2', 'qwer1234!', now());

